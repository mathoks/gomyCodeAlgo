ALGORITHM sum_char_string
VAR
    char_sum, word_sum : INTEGER;
    imput_string: STRING;
BEGIN
    initialize char_sum, word_sum = 0;
    Read(imput_string)
    iteration: 
    FOR  i FROM 0  TO length(imput_string) STEP 1 DO;
    IF imput_string if CHAR != " "
    Write char_sum++;
    if CHAR === "."
    Write char_sum;

END